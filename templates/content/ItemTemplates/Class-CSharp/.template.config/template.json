{
  "$schema": "http://json.schemastore.org/template",
  "author": "Next Level Tribe",
  "description": "Creates a new C# class.",
  "classifications": [ "Common" ],
  "groupIdentity": "NextLevelTribe.Class",
  "precedence": "10",
  "identity": "NextLevelTribe.Class.CSharp",
  "name": "Class",
  "shortName": "nlt-class",
  "sourceName": "Class1",
  "preferDefaultName": true,
  "defaultName": "Class1",
  "primaryOutputs": [
    {
      "path": "Class1.cs"
    }
  ],
  "tags": {
    "language": "C#",
    "type": "item"
  },
  "symbols": {
    "DefaultNamespace": {
    "type": "bind",
    "binding": "msbuild:RootNamespace",
    "replaces": "Company.ClassLibrary1",
    "defaultValue": "Company.ClassLibrary1"
    }
  }
}